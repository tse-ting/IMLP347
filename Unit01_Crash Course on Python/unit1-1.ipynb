    "#樹入一個數字，判斷為奇偶數\n",
    "num = int(input())\n",
    "if num > 1 and num<=1000:\n",
    "    if num % 2 == 0:\n",
    "        print(\"他是偶數\")\n",
    "    else:\n",
    "        print(\"他是奇數\")\n",
    "else:\n",
    "    print(\"超出範圍\")"
